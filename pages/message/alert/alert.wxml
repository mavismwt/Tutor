<!--pages/message/alert/alert.wxml-->
<view>
  <navigationBar id="navigationBar" title="定时报警" back="true"></navigationBar> 
  <view class="container {{IPX?'IPX':''}}">
  <view class="section">
    <view class="section__title">时间选择器</view>
    <picker mode="time" value="{{time}}" start="09:01" end="21:01" bindchange="bindTimeChange">
      <view class="picker">
        当前选择: {{time}}
      </view>
    </picker>
  </view>
  <view catchtap='cancel'>
    <definedButton title="取消"style='margin-top:100px'></definedButton>
  </view>
</view>
</view>