<view>
<navigationBar id="navigationBar" title="学生列表"></navigationBar>    
  <view class="container">
    <view class="selLine" style="position:fixed">
      <button class='button'>家教类型
      </button>
      <button class='button'>科目
      </button>
      <button class='button'>年级
      </button>
      <button class='button'>性别要求
      </button>
    </view>
    <view style="margin-top:50px;margin-bottom:60px;">
      <studentList id="studentList"  wx:for="{{studentArray}}" wx:key="unique" bindtap="navToDetail"
      name = "{{item.name}}"
      price="{{item.price}}"
      grade="{{item.grade}}"
      object="{{item.object}}"
      time="{{item.time}}"
      location="{{item.location}}"
      sex="{{item.sex}}"
      isLongTerm="{{item.isLongTerm}}"
      number="{{item.number}}"
      perTime="{{item.perTime}}"
      bindtap='navToDetail'>
      </studentList>
    </view>
    <tabbar tabbar="{{tabbar}}"></tabbar>
  </view>
</view>