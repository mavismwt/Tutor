<view>
  <navigationBar id="navigationBar" title="填写资料"back="true"></navigationBar> 
  <view class='container'>
      <view class='select-view'>
        <view class='select' bindtap='teacherAuth'>
          <image class='icon' src='/images/icon/icon_teacher.png'></image>
          <view class='title'>当家教</view>
        </view>
        <view class='select' bindtap='studentAuth'>
          <image class='icon' src='/images/icon/icon_student.png'></image>
          <view class='title'>找家教</view>
        </view>
      </view>
      <text class='more-info'>认证后才能发送申请\n9:00-22:00三小时内审核完毕</text>
  </view>
</view>
