<!--pages/mine/collect/collect.wxml-->
<view>
  <navigationBar id="navigationBar" title=""back="true"></navigationBar> 
  <view class='container'>
  <view style="margin-top:5px;">
      <studentList id="studentList"  wx:for="{{studentArray}}" wx:key="unique" wx:if="{{identity=='teacher'}}"
      name = "{{item.name}}"
      img = "{{item.img}}"
      price="{{item.price}}"
      grade="{{item.grade}}"
      object="{{item.object}}"
      time="{{item.time}}"
      location="{{item.location}}"
      sex="{{item.sex}}"
      isLongTerm="{{item.isLongTerm}}"
      number="{{item.number}}"
      perTime="{{item.perTime}}"
      bindtap='onClick'
      id="{{index}}">
      </studentList>
      <teacherList id="studentList"  wx:for="{{teacherArray}}" wx:key="unique" wx:if="{{identity=='student'}}"
      name = "{{item.name}}"
      img = "{{item.img}}"
      price="{{item.price}}"
      school = "{{item.school}}"
      grade="{{item.grade}}"
      object="{{item.object}}"
      time="{{item.time}}"
      location="{{item.location}}"
      sex="{{item.sex}}"
      bindtap='onClickT'
      id="{{index}}">
      </teacherList>
    </view>
  </view>
</view>
