/* pages/mine/mine.wxss */
.container {
  display: flex;
  padding: 0px 0px 0px 0px;
  background-color: #f5f5f5;
}
.sel-view {
  background-color: #f5f5f5;
}
.selLine {
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 50px;
  align-items: flex-start;
  background-color: white;
  box-shadow: 0px 2px 4px 0px 
		rgba(24, 99, 97, 0.1);
}

.button {
  display: flex;
  flex-direction: row;
  width: 25%;
  height: 40px;
  margin-top: 5px;
  align-items: flex-start;
}

.listView {
  display: list-item;
  flex-direction: column;
  width: 100%;
}
.cover-view {
  position: fixed;
  display: flex;
  margin-top: 135px;
  width: 100vw;
  height: 100vh;
  flex-direction: column;
  background-color: #f5f5f5;

}
.objects {
  clear: both;
  width: 90vw;
  display: inline-block;
  margin-left: 5vw;
  margin-top: 25%;
  align-items: center;
}
.object-item {
  float: left;
  font-size: 16px;
  width: 28%;
  padding: 10px 0px;
  text-align: center;
  margin-left: 4%;
  margin-top: 20px;
  border: 1px solid #3cdede;
  border-radius: 5px;
  color: #3cdede;
}
.confirm {
  float: left;
  font-size: 16px;
  width: 40vw;
  padding: 10px 0px;
  text-align: center;
  margin-left: 30vw;
  margin-top: 40px;
  border-radius: 20px;
  background-color: #3cdede;
  color: #fff;
}
