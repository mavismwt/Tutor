/* pages/middle/middle.wxss */
.container {
  position: fixed;
  display: flex;
  height: 100vh;
  width: 100vw;
}
.select-view {
  display: flex;
  flex-direction: row;
  margin-top: 30vh;
  align-items: center;
  justify-content: center;
}
.select {
  display: flex;
  flex-direction: column;
}
.icon {
  width: 90px;
  height: 90px;
  padding: 20px;
}
.title {
  font-size: 17px;
  text-align: center;
  color: #b2b2b2;
}
